{
  "version": 2, 
  "builds": [
    { "src": "api/index.php", "use": "vercel-php@0.6.0" }
  ],
  "functions": {
      "api/index.php": {
        "runtime": "vercel-php@0.7.1"
      }
    },
  "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ],
  "env": {
        "APP_NAME": "deploy-calificaciones",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://deploy-calificaciones.vercel.app"
    }
}

  